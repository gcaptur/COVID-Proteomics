#wilcoxon test for diff btw seropositive and uninfected example

a2 <- read.csv("~/Documents/PUBLICATIONS/PC/Publications/_COVID_PROTEOMICS/STATS/__FINAL_MERGE/nicvspcr.csv")
a2$group<-as.factor(a2$group)
wilcox.test(a2$age~a2$group, data=a2)

a2 <- read.csv("~/Documents/PUBLICATIONS/PC/Publications/_COVID_PROTEOMICS/STATS/__FINAL_MERGE/analytes.csv")
a2$SPP2_DAL<-as.numeric(a2$SPP2_DAL)
a2$PTGDS_TML<-as.numeric(a2$PTGDS_TML)
a2$NRP1_ATH<-as.numeric(a2$NRP1_ATH)
a2$GRN_AVA<-as.numeric(a2$GRN_AVA)
a2$CDKL5_GTS<-as.numeric(a2$CDKL5_GTS)

wilcox.test(a2$A1AT_LSS~a2$PCR, data=a2)
wilcox.test(a2$A2AP_HQM~a2$PCR, data=a2)
wilcox.test(a2$A2AP_LGN~a2$PCR, data=a2)
