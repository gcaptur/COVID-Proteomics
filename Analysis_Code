#Log2 enrichment plot (volcano p values were calculated first)

tmp<-read.csv("~/Documents/PUBLICATIONS/PC/Publications/_COVID_PROTEOMICS/STATS/__FINAL_MERGE/volcanoplot1.csv")
tmp=read.csv("~/Documents/PUBLICATIONS/PC/Publications/_COVID_PROTEOMICS/STATS/__FINAL_MERGE/volcanoplot1.csv")
tmp<- data.frame(tmp[,-1], row.names=tmp[,1])
de<-tmp
ggplot(data=de, aes(x=log2FoldChange, y=pvalue)) + geom_point()
p <- ggplot(data=de, aes(x=log2FoldChange, y=-log10(pvalue))) + geom_point(shape=1, fill="#800020", alpha = 1/10, color="#800020", size=2)
p
p <- ggplot(data=de, aes(x=log2FoldChange, y=-log10(pvalue))) + geom_point(shape=21,fill="#800020", alpha = 1/10, size=2, color="#800020") + theme_minimal() + geom_point(shape=21, size=2, color="#800020") 
p2 <- p + geom_vline(xintercept=c(-0.6, 0.6), col="#D3D3D3", linetype = "dashed") +
    geom_hline(yintercept=-log10(0.05), col="#D3D3D3", linetype = "dashed")
p2
de$diffexpressed <- "NO"
de$diffexpressed[de$log2FoldChange > 0.6 & de$pvalue < 0.05] <- "UP"
de$diffexpressed[de$log2FoldChange < -0.6 & de$pvalue < 0.05] <- "DOWN"
p3 <- p2 + scale_color_manual(values=c("#800020", "#800020", "grey"))
p3
mycolors <- c("grey", "#800020", "#800020")
names(mycolors) <- c("NO", "UP", "DOWN")
p3 <- p2 + scale_colour_manual(values = mycolors)

ggplot(data=de, aes(x=log2FoldChange, y=-log10(pvalue), col=diffexpressed, label=gene_symbol, fill = diffexpressed)) +
        geom_point(shape=21, alpha = 3/10, size=2)  +
        theme_minimal() +
        geom_text_repel(size = 2) +
        scale_fill_manual(values=c("#800020", "grey", "#800020")) + 
        scale_color_manual(values=c("#34000d", "grey", "#34000d")) +
        geom_vline(xintercept=c(-0.6, 0.6), col="#D3D3D3", linetype = "dashed") +
        geom_hline(yintercept=-log10(0.05), col="#D3D3D3", linetype = "dashed")

